<UserControl x:Class="BMHRI.WCS.Server.UserControls.SenceUCMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BMHRI.WCS.Server.UserControls"
             mc:Ignorable="d" 
             xmlns:ValueConverter="clr-namespace:BMHRI.WCS.Server.ValueConverter"
              xmlns:UserControls="clr-namespace:BMHRI.WCS.Server.UserControls"
             Loaded="UserControl_Loaded"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid >
        <TabControl TabStripPlacement="Bottom" Style="{StaticResource TabControlWithUnderLineStyle}" SelectionChanged="TabControl_SelectionChanged" Background="#FAF3E0" BorderBrush="#FAF3E0" >
            <TabItem Header="1" FontSize="20" Foreground="#ddacacac" Style="{StaticResource TabItemExWithUnderLineStyle}" Height="30" Width="30" >
                <UserControls:SenceUC Loaded="SenceUC_Loaded" />
            </TabItem>
            <TabItem Header="1.5" FontSize="20" Foreground="#ddacacac" Style="{StaticResource TabItemExWithUnderLineStyle}" Height="30" Width="40" >
                <UserControls:SenceUC_4 Loaded="SenceUC_4_Loaded" />
            </TabItem>
            <TabItem Header="2" FontSize="20" Foreground="#ddacacac" Style="{StaticResource TabItemExWithUnderLineStyle}" Height="30" Width="30" >
                <UserControls:SenceUC_2 Loaded="SenceUC_2_Loaded" />
            </TabItem>
            <TabItem Header="3" FontSize="20" Foreground="#ddacacac" Style="{StaticResource TabItemExWithUnderLineStyle}" Height="30" Width="30" >
                <UserControls:SenceUC_3 Loaded="SenceUC_3_Loaded" />
            </TabItem>
            
        </TabControl>
    </Grid>
</UserControl>
